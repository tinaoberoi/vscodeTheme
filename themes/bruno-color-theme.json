{
	"name": "bruno",
	"colors": {
		"editor.background": "#2C001E",
		"editor.foreground": "#e3632cf1",
		// Terminal styles with a grayish or translucent background
		// Terminal styles with a complementary gray background
		"terminal.background": "#4B3B47", // Gray with subtle purple undertones
		"terminal.foreground": "#A6E22E", // Mellow green text
		"terminalCursor.background": "#A6E22E", // Mellow green cursor background
		"terminalCursor.foreground": "#A6E22E", // Mellow green cursor foreground
		"terminal.ansiBlack": "#2E2E2E", // Slightly darker gray for black
		"terminal.ansiRed": "#D16969", // Muted red for errors
		"terminal.ansiGreen": "#A6E22E", // Mellow green for success messages
		"terminal.ansiYellow": "#E6DB74", // Muted yellow for warnings
		"terminal.ansiBlue": "#66D9EF", // Muted blue for informational messages
		"terminal.ansiMagenta": "#AE81FF", // Muted magenta
		"terminal.ansiCyan": "#A1EFE4", // Muted cyan
		"terminal.ansiWhite": "#F8F8F2", // Soft white for general text
		"terminal.ansiBrightBlack": "#75715E", // Bright gray for bright black
		"terminal.ansiBrightRed": "#F92672", // Mellowed bright red
		"terminal.ansiBrightGreen": "#A6E22E", // Mellowed bright green
		"terminal.ansiBrightYellow": "#E6DB74", // Mellowed bright yellow
		"terminal.ansiBrightBlue": "#66D9EF", // Mellowed bright blue
		"terminal.ansiBrightMagenta": "#AE81FF", // Mellowed bright magenta
		"terminal.ansiBrightCyan": "#A1EFE4", // Mellowed bright cyan
		"terminal.ansiBrightWhite": "#F8F8F2", // Soft bright white
		// Editor styles
		"editorCursor.foreground": "#32e30e",
		"editorError.foreground": "#FF0000", // Bright red for the error squiggly line
		"editorError.border": "#FF4500", // A strong orange-red border for errors
		// Active tab styles
		"tab.activeBackground": "#2E2E2E", // Dark gray for active tab background
		"tab.activeForeground": "#FFFFFF", // Keep text white for good contrast
		"tab.inactiveBackground": "#3B1F35", // Lighter gray or purple for inactive tabs
		"tab.inactiveForeground": "#BBBBBB", // Light gray text for inactive tabs
		// "tab.activeBorder": "#E95420",           // Bright Orange for border emphasis
		// "tab.activeBorderTop": "#E95420",        // Bright Orange top border
		// Inactive tab styles
		// Unfocused tabs
		"tab.unfocusedActiveBackground": "#2E2E2E", // Dark gray for unfocused active tab
		"tab.unfocusedInactiveBackground": "#3B1F35", // Darker gray for unfocused inactive tabs
		"tab.unfocusedActiveForeground": "#BBBBBB", // Light gray text for unfocused active tab
		"tab.unfocusedInactiveForeground": "#777777", // Dimmed text for unfocused inactive tabs
		// Explorer / Files area styles
		"sideBar.background": "#3B1F35", // Lighter background color for the Explorer panel
		"sideBar.foreground": "#E0E0E0", // Foreground (text) color for file and folder names
		"sideBar.border": "#440530", // Border color for the Explorer panel
		"sideBarSectionHeader.background": "#573C50", // Background color of section headers in the sidebar
		"sideBarSectionHeader.foreground": "#FFFFFF", // Text color of section headers
		"list.activeSelectionBackground": "#673320", // Background color of selected/active item
		"list.activeSelectionForeground": "#FFFFFF", // Text color of selected/active item
		"list.inactiveSelectionBackground": "#573C50", // Background color of selected item when inactive
		"list.inactiveSelectionForeground": "#BBBBBB", // Text color of selected item when inactive
		"list.hoverBackground": "#673320", // Background color when hovering over items
		"list.hoverForeground": "#FFFFFF", // Text color when hovering over items
		"list.focusBackground": "#E95420", // Background color for focused item
		"list.focusForeground": "#FFFFFF", // Text color for focused item
		"list.highlightForeground": "#FFD700", // Text color for highlighted items (e.g., matched search results)
		"list.dropBackground": "#573C30", // Background color when dragging an item over another item
		"tree.indentGuidesStroke": "#BBBBBB", // Color of the indentation guides in the file tree
		"tree.tableColumnsBorder": "#573C50", // Border color for table columns (if used in Explorer)
		"tree.tableOddRowsBackground": "#3B1F35" // Background for odd rows in the tree view
	},
	"tokenColors": [
		{
			"scope": "string.quoted.docstring",
			"settings": {
				"foreground": "#4EC9B0", // A distinct, light gold color
				"fontStyle": "italic"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#1607db"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6A9955"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#290ccc"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#b46695"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": [
				"source.css entity.other.attribute-name",
				"source.css.less entity.other.attribute-name.id",
