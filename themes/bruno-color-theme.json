{
	"name": "bruno",
	"colors": {
		"editor.background": "#3B1F35",
		"editor.foreground": "#E6E6FA",
		// Terminal styles with a grayish or translucent background
		"terminal.background": "#4B3B47", // Gray with subtle purple undertones
		// Editor styles
		"editorCursor.foreground": "#32e30e",
		// Errors and warnings
		"editorError.foreground": "#FF0000", // Bright red for the error squiggly line
		"editorError.border": "#FF0000", // A strong red border for errors
		// Active tab styles
		"tab.activeBackground": "#2E2E2E", // Dark gray for active tab background
		"tab.activeForeground": "#FFFFFF", // Keep text white for good contrast
		"tab.inactiveBackground": "#3B1F35", // Lighter gray or purple for inactive tabs
		"tab.inactiveForeground": "#BBBBBB", // Light gray text for inactive tabs
		// Unfocused tabs
		"tab.unfocusedActiveBackground": "#2E2E2E", // Dark gray for unfocused active tab
		"tab.unfocusedInactiveBackground": "#3B1F35", // Darker gray for unfocused inactive tabs
		"tab.unfocusedActiveForeground": "#BBBBBB", // Light gray text for unfocused active tab
		"tab.unfocusedInactiveForeground": "#777777", // Dimmed text for unfocused inactive tabs
		// Explorer / Files area styles
		"sideBar.background": "#2C001E", // Lighter background color for the Explorer panel
		"sideBar.foreground": "#E0E0E0", // Foreground (text) color for file and folder names
		"sideBar.border": "#440530", // Border color for the Explorer panel
		"sideBarSectionHeader.background": "#573C50", // Background color of section headers in the sidebar
		"sideBarSectionHeader.foreground": "#FFFFFF", // Text color of section headers
		"list.activeSelectionBackground": "#4B3B47", // Muted purple background for selected/active item
		"list.activeSelectionForeground": "#FFFFFF", // Text color of selected/active item
		"list.inactiveSelectionBackground": "#573C50", // Background color of selected item when inactive
		"list.inactiveSelectionForeground": "#BBBBBB", // Text color of selected item when inactive
		"list.hoverBackground": "#4B3B47", // Muted purple background when hovering over items
		"list.hoverForeground": "#FFFFFF", // Text color when hovering over items
		"list.focusBackground": "#5A4E61", // Slightly lighter purple for focused item
		"list.focusForeground": "#FFFFFF", // Text color for focused item
		"list.highlightForeground": "#66D9EF", // Muted blue for highlighted items (e.g., matched search results)
		"list.dropBackground": "#573C50", // Background color when dragging an item over another item
		"tree.indentGuidesStroke": "#BBBBBB", // Color of the indentation guides in the file tree
		"tree.tableColumnsBorder": "#573C50", // Border color for table columns (if used in Explorer)
		"tree.tableOddRowsBackground": "#3B1F35" // Background for odd rows in the tree view
	},
	"tokenColors": [
		{
			"scope": "string.quoted.docstring",
			"settings": {
				"foreground": "#4EC9B0", // A distinct, light gold color
				"fontStyle": "italic"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#1607db"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7A7A7A"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#9370DB"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#b46695"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": [
				"source.css entity.other.attribute-name",
				"source.css.less entity.other.attribute-name.id",
				"source.scss entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d16969"
			}
		},
		// Markdown-specific styles
		{
			"name": "Markdown headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#00FF7F", // Bright green (SpringGreen) for headings
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown bold",
			"scope": "markup.bold",
			"settings": {
				"foreground": "#FFFFFF", // White color for bold text
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown italic",
			"scope": "markup.italic",
			"settings": {
				"foreground": "#BBBBBB", // Light gray for italic text
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown blockquote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#87CEEB", // Sky blue for blockquotes
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown code blocks",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#FFD700", // Gold color for inline code blocks
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown links",
			"scope": "markup.underline.link",
			"settings": {
				"foreground": "#00BFFF", // DeepSkyBlue for links
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markdown lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#C586C0", // Light purple for list items
				"fontStyle": "normal"
			}
		},
		{
			"name": "Markdown code fences",
			"scope": "markup.fenced_code.block",
			"settings": {
				"foreground": "#FFD700", // Gold color for fenced code blocks
				"fontStyle": "bold"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "coloring of the TS this",
			"scope": "variable.language.this",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#DCDCAA"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",
				"source.cs storage.type"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"name": "Control flow keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#9CDCFE"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#9CDCFE"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#CE9178"
			}
		},
		// Class and Struct Member Access
		{
			"name": "Class/Struct Member Access",
			"scope": [
				"variable.other.property.ts", // TypeScript/JavaScript class property access
				"variable.other.property.js", // JavaScript class property access
				"variable.other.property.python", // Python class/struct property access
				"variable.other.member.cpp" // C++ struct/class member access
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		// Global Variable Definitions
		{
			"name": "Global Variable Definitions",
			"scope": [
				"variable.other.readwrite.global", // Common for global variables
				"meta.definition.variable.name" // Variable name in definition
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
	]
}